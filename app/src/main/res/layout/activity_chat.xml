<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="me.aflak.libraries.ui.scan.view.ScanActivity"
    android:orientation="vertical">

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="254dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_alignEnd="@+id/activity_chat_status"
        android:layout_alignRight="@+id/activity_chat_status"
        android:layout_marginStart="124dp"
        android:layout_marginLeft="124dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:max="2500"
        android:progress="100" />

    <TextView
        android:id="@+id/valueCH1off"
        android:layout_width="42dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_marginStart="54dp"
        android:layout_marginLeft="54dp"
        android:layout_marginTop="48dp"
        android:text="100"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:typeface="monospace"
        app:fontFamily="sans-serif-medium" />

    <TextView
        android:id="@+id/activity_chat_status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/activity_chat_messages"
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/activity_chat_hello_world"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="450dp"
        android:layout_marginBottom="0dp"
        android:scrollbars="vertical"
        android:textSize="18dp" />

    <Button
        android:id="@+id/activity_chat_hello_world"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/helloWorld"
        android:layout_alignParentBottom="true"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="3dp"
        android:text="CH1:" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="4dp"
        android:text="CH1 on:" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="3dp"
        android:text="CH1 off:" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
        android:layout_toEndOf="@+id/valueCH1"
        android:layout_toRightOf="@+id/valueCH1"
        android:text="mV" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_marginStart="98dp"
        android:layout_marginLeft="98dp"
        android:layout_marginTop="25dp"
        android:text="mV" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_marginStart="98dp"
        android:layout_marginLeft="98dp"
        android:layout_marginTop="48dp"
        android:text="mV" />

    <TextView
        android:id="@+id/valueCH1"
        android:layout_width="42dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_marginStart="53dp"
        android:layout_marginLeft="53dp"
        android:layout_marginTop="3dp"
        android:text="1250"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:typeface="monospace"
        app:fontFamily="sans-serif-medium" />

    <TextView
        android:id="@+id/valueCH1on"
        android:layout_width="42dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_marginStart="53dp"
        android:layout_marginLeft="53dp"
        android:layout_marginTop="26dp"
        android:text="2500"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:typeface="monospace"
        app:fontFamily="sans-serif-medium" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="254dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_chat_status"
        android:layout_alignStart="@+id/activity_chat_status"
        android:layout_alignLeft="@+id/activity_chat_status"
        android:layout_alignEnd="@+id/activity_chat_status"
        android:layout_alignRight="@+id/activity_chat_status"
        android:layout_marginStart="124dp"
        android:layout_marginLeft="124dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:max="2500"
        android:progress="2500" />
</RelativeLayout>
